{
  "name": "visioapp",
  "version": "0.0.1",
  "description": "Aplicación para Visio Oftalmología Veterinaria",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/irodgal/visioapp"
  },
  "dependencies": {
    "angular": "^1.5.5",
    "angular-local-storage": "^0.2.7",
    "angular-resource": "^1.5.5",
    "angular-ui-bootstrap": "^1.3.2",
    "angular-ui-router": "^0.2.18",
    "body-parser": "^1.15.0",
    "bootstrap": "^3.3.6",
    "cloudant": "^1.4.1",
    "compression": "^1.6.1",
    "dotenv": "^2.0.0",
    "express": "^4.13.4",
    "glob": "^7.0.3",
    "jsonwebtoken": "^5.7.0",
    "lodash": "^4.6.1",
    "method-override": "^2.3.5",
    "nano": "^6.2.0",
    "path": "^0.12.7",
    "pdfkit": "^0.7.2"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.5",
    "brfs": "^1.4.3",
    "browserify": "^13.0.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "clean-css": "^3.4.12",
    "coveralls": "^2.11.9",
    "istanbul": "^0.4.2",
    "istanbul-combine": "^0.3.0",
    "karma": "^0.13.22",
    "karma-browserify": "^5.0.5",
    "karma-chai": "^0.1.0",
    "karma-chai-as-promised": "^0.1.2",
    "karma-coverage": "^1.0.0",
    "karma-mocha": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon-chai": "^1.2.0",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.2.0",
    "mustache": "^2.2.1",
    "node-sass": "^3.7.0",
    "nodemon": "^1.9.2",
    "phantomjs-prebuilt": "^2.1.5",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "supertest": "^1.2.0",
    "uglify-js": "^1.3.5",
    "watchify": "^3.7.0"
  },
  "scripts": {
    "start": "node app.js",
    "start-debug": "node --debug app.js",
    "debugger": "node-inspector --web-port=18080 app.js",
    "build-css": "node-sass --include-path scss public/scss/main.scss public/css/main.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "minify-css": "cat ./public/css/*.css | cleancss -o public/dist/visioapp.min.css",
    "minify-css-win": "type public\\css\\*.css | cleancss -o public/dist/visioapp.min.css",
    "minify-js": "node bin/minify.js",
    "build": "node bin/build.js",
    "test": "mocha -R spec",
    "test-karma": "karma start karma.conf.js",
    "test-coverage": "istanbul cover _mocha -- --recursive",
    "istanbul-combine": "istanbul-combine -d ./coverage/combine -r lcov ./coverage/coverage.json ./coverage/karma/report-json/coverage-final.json",
    "coveralls": "cat ./coverage/combine/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "author": "Iván R. Galicia",
  "license": "MIT"
}
